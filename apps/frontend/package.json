{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run typecheck && vite build",
    "dev": "vite --host",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "lint": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "serve": "npm run build && vite preview --port 3000",
    "typecheck": "tsc --noEmit",
    "generate": "graphql-codegen --require dotenv/config --config codegen.yml dotenv_config_path=.env",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf pnpm-lock.yaml"
  },
  "lint-staged": {
    "*.{css,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.10",
    "@tanstack/react-query": "^4.12.0",
    "@types/lodash.debounce": "^4.0.7",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^3.0.0",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/typescript": "^2.7.4",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@graphql-codegen/typescript-react-query": "^4.0.2",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-react": "^1.3.2",
    "commitizen": "^4.2.4",
    "eslint-config-custom": "workspace:*",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "sort-package-json": "^1.57.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.5",
    "tsconfig": "workspace:*",
    "typescript": "^4.8.4",
    "vite": "^2.9.12",
    "vite-plugin-graphql-codegen": "^2.3.1"
  }
}
